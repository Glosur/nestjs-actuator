{
  "name": "nest-actuator",
  "version": "0.0.1",
  "description": "Integration with Spring Boot Admin for NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Glosur/nest-actuator.git"
  },
  "author": "Glosur <s.blanc@hotmail.fr>",
  "license": "MIT",
  "keywords": [
    "nest",
    "nestjs",
    "monitoring",
    "spring-boot-admin"
  ],
  "dependencies": {
    "@nestjs/common": "7.0.13",
    "@nestjs/core": "7.0.13",
    "@nestjs/schedule": "^0.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.5",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/testing": "7.0.13",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.3",
    "@types/node": "13.13.7",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "reflect-metadata": "0.1.13",
    "jest": "25.5.4",
    "lint-staged": "10.2.4",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "25.5.1",
    "ts-loader": "7.0.4",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.2"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watch",
    "build": "rm -rf ./dist && tsc --declaration",
    "format": "prettier src/**/*.ts --ignore-path ./.prettierignore --write && git status",
    "prepublish": "npm run format && npm run build"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/Glosur/nest-actuator/issues"
  },
  "homepage": "https://github.com/Glosur/nest-actuator#readme",
  "directories": {
    "example": "examples"
  }
}
